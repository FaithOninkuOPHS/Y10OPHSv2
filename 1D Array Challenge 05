food = []
food1 = str(input("Enter a food >>>"))
food2 = str(input("Enter another food >>>"))
food3 = str(input("Enter another food >>>"))
food4 = str(input("Enter another food >>>"))

food.append(food1)
food.append(food2)
food.append(food3)
food.append(food4)

print(food)

item = int(input("Enter number of item to delete >>> "))
item = item-1

del food[item]

print(food)
